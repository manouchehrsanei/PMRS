<?xml version="1.0" ?>
    <!-- Simulating an Terzaghi poroelastic three dimesnional  -->
<CaseData>
    <!-- Geometry description -->
    <Mesh>
       <MeshFile mesh_file="/Users/manouchehr/Documents/GitHub/PMRS/input/meshes/Terzaghi_3D.msh" />
    </Mesh>
    <!-- Time step controls  -->
    <TimeControls>
        <StepSize dt="0.001" />
        <StepNumber n_time_steps="10" />
    </TimeControls>
    <!-- Newton method controls  -->
    <NewtonControls>
        <Iterations n_iterations="1" />
        <Residue res_tolerance="0.01" />
        <Correction cor_tolerance="0.01" />
    </NewtonControls> 
        <!-- Parallel controls  -->
    <ParallelControls>
        <Numthreads n_threads="4" />
    </ParallelControls> 
    <!-- Finite element controls  -->
    <FEM>
        <MixedFormulationQ useQ="0" />
        <HRefine h_level="0" />
        <Elasticity p_order="3" />
        <Diffusion p_order="2" />
    </FEM>
    <!-- Settings for OutPut -->
    <OutputControls>
        <OutputFolder name="mkdir Output" />
        <LogFolder name="mkdir Log" />
        <PostProcessing vtk_file="Terzaghi_3D.vtk" n_divisions="1" n_scalars="1" n_vectors="2"
        />
        <Scalars>
            <Var name="p" />
        </Scalars>
        <Vectors>
            <Var name="u" />
            <Var name="v" />
        </Vectors>
    </OutputControls>
    <Physics>
        <GravityConstant gravity="0.0" />
        <GravityDirection x_direction="0.0" y_direction="0.0" z_direction="-1.0" />
        <Prestress sxx= "0.0" syy= "0.0" szz= "0.0" sxy= "0.0" sxz= "0.0" syz= "0.0" />
    </Physics>
    <!-- Material Settings -->
    <ReservoirRegions>
        <RegionNumber n_regions="1" />
    </ReservoirRegions> 
    <RegionsDefinition>
        <!-- Region definition -->
        <RegionData mat_id="1" n_boundaries="3" n_parameters="12" >
            <Parameters eyoung="0.9" nu="0.2" phi="1.0" kappa="1.0" alpha="1.0" m="1.0e29" mu="1.0" rhof="1.0" rhos="1.0"
            MCcoh="0.0" MCphi="0.0" MCpsi="0.0"/>
            <Boundaries>
                <Boundary bc_id="2"/>
                <Boundary bc_id="3"/>
                <Boundary bc_id="4"/>
            </Boundaries>
        </RegionData>          
    </RegionsDefinition>
    <BoundaryConditions>
        <BoundaryCondition bc_id="2" type="Duz_Nq" uz="0.0" qn="0.0" />
        <BoundaryCondition bc_id="3" type="Nt_Dp" tx="0.0" ty="0.0" tz="-1.0" p="0.0" />
        <BoundaryCondition bc_id="4" type="Duxy_Nq" ux="0.0" uy="0.0" qn="0.0" />
    </BoundaryConditions>           
</CaseData>
