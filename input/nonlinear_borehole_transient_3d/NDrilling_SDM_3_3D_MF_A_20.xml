<?xml version="1.0" ?>
    <!-- Example 1: Simulating poroelastoplastic case -->
<CaseData>
    <!-- Geometry description -->
    <Mesh>
       <MeshFile mesh_file="vertical_wellbore_3D_5_inches_50k_geo.msh" />
    </Mesh>
    <!-- Time step controls  -->
    <TimeControls>
        <StepSize dt="2.0" />
        <StepNumber n_time_steps="50" />
        <CrankNicolsonQ useQ="0" />
    </TimeControls>
    <!-- Newton method controls  -->
    <NewtonControls>
        <Iterations n_iterations="80" />
        <Residue res_tolerance="0.0001" />
        <Correction cor_tolerance="0.0001" />
    </NewtonControls>
    <!-- Fixed Stress Split Scheme  -->
    <FixedStressSplit>
        <FssIterations n_max_fss_iterations="50" />
        <EnfFssIterations n_enforce_fss_iterations="0" />
        <NonlinearAcceleration n_nonlinear_acceleration="SDM" /> <!-- n_nonlinear_acceleration = {None,FDM,SDM}  -->
        <NState n_state="6" /> <!-- NState = {2,4,6}  -->
        <MaxThetaRatio max_theta_value="0.5" /> <!-- max_theta_value = {0.0-1.0}  -->
    </FixedStressSplit>
    <!-- SubSteps controls  -->
    <SubSteps>
        <MaxPlasticNorm max_plastic_norm_value="0.5" />
    </SubSteps>  
    <!-- Initialization controls  -->
    <Initialization>
        <ResetUndarinedResposeData reset_undrained_response_data="0" />
    </Initialization>      
    <!-- Parallel controls  -->
    <ParallelControls>
        <Numthreads n_threads="64" />
    </ParallelControls>
    <!-- Scale factor controls  -->
    <ScaleFactor>
        <Valscalefactor scalfac_value="1.0e-6" />
    </ScaleFactor>    
    <!-- Finite element controls  -->
    <FEM>
        <FullyCoupled useQs="0" />
        <MixedFormulationQ useQ="1" />
        <HRefine h_level="0" />
        <Elasticity p_order="2" />
        <Diffusion p_order="1" />
    </FEM>
    <!-- Settings for OutPut -->
    <OutputControls>
        <OutputFolder name="mkdir Output" />
        <LogFolder name="mkdir Log" />
        <PostProcessing vtk_file="NDrilling_SDM_2_3D_MF_A_20" n_divisions="0" n_outputs_geo="6" n_outputs_res="5" draw_initial_data_Q="1" draw_geometry_Q="1" performance_summary_Q="1" />
        <OutputControlsGeo>
            <Var s_name="alpha" />
            <Var v_name="u" />
            <Var t_name="s" />
            <Var t_name="s_t" />            
            <Var t_name="e" />
            <Var t_name="ep" />
        </OutputControlsGeo>
        <OutputControlsRes>
            <Var s_name="div_q" />
            <Var s_name="p" />
            <Var s_name="phi" />
            <Var s_name="kappa" />
            <Var v_name="q" />            
        </OutputControlsRes> 
        <ReportingTimes>
            <time t="10.0" />
            <time t="20.0" />
            <time t="30.0" />
            <time t="40.0" />
            <time t="50.0" />
            <time t="60.0" />
            <time t="70.0" />
            <time t="80.0" />
            <time t="90.0" />
            <time t="100.0" />
        </ReportingTimes>         
    </OutputControls>
    <Physics>
