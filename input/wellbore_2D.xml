<?xml version="1.0" ?>
    <!-- Example 1: Simulating a case similar to SEST2D -->
<CaseData>
    <!-- Geometry description -->
    <Mesh>
       <MeshFile mesh_file="/Users/omar/Documents/GitHub/PMRS/input/meshes/Wellbore_Cricular_2D_SEST2D.msh" />
    </Mesh>
    <!-- Time step controls  -->
    <TimeControls>
        <StepSize dt="1.0" />
        <StepNumber n_time_steps="10" />
        <CrankNicolsonQ useQ="0" />
    </TimeControls>
    <!-- Newton method controls  -->
    <NewtonControls>
        <Iterations n_iterations="40" />
        <Residue res_tolerance="0.001" />
        <Correction cor_tolerance="0.001" />
    </NewtonControls>
    <!-- Parallel controls  -->
    <ParallelControls>
        <Numthreads n_threads="0" />
    </ParallelControls>  
    <!-- Finite element controls  -->
    <FEM>
        <MixedFormulationQ useQ="0" />
        <HRefine h_level="0" />
        <Elasticity p_order="2" />
        <Diffusion p_order="1" />
    </FEM>
    <!-- Settings for OutPut -->
    <OutputControls>
        <OutputFolder name="mkdir Output" />
        <LogFolder name="mkdir Log" />
        <PostProcessing vtk_file="SEST_2D" n_divisions="1" n_outputs_geo="7" n_outputs_res="1" />
        <OutputControlsGeo>
            <Var s_name="e_x" />
            <Var s_name="e_y" />
            <Var s_name="phi" />
            <Var s_name="k_x" />
            <Var s_name="k_y" />
            <Var s_name="s_x" />
            <Var s_name="s_y" />
        </OutputControlsGeo>
        <OutputControlsRes>
            <Var s_name="p" />
        </OutputControlsRes>        
    </OutputControls>
    <Physics>
        <GravityConstant gravity="0.0" />
        <GravityDirection x_direction="0.0" y_direction="0.0" z_direction="-1.0" />
    </Physics>
    <!-- Material Settings -->
    <ReservoirRegions>
        <RegionNumber n_regions="1" />
    </ReservoirRegions> 
    <RegionsDefinition>
        <!-- Region definition -->
        <RegionData mat_id="1" n_boundaries_geo="4" n_boundaries_res ="4">
            <PoroMechUndrainedParameters Eyoung_u="36495.0" nu_u="0.4999" phi_0="0.1" kappa_0="1.0e-11" />            
            <PoroMechParameters Eyoung="29269.0" nu="0.20300" alpha="1.0" Se="1.0e-8" eta="0.001" rho_f="1000.0" rho_s="2700.0" />
            <PhiParameters phi_model="Linear" n_parameters="0"/>
            <KappaParameters kappa_model="Davies" n_parameters="1" c="0.1"/>
            <PlasticityParameters plasticity_model="MC" n_parameters="2" cohesion="30" friction="1.0"/>            
            <GeoBoundaries>
                <Boundary bc_id="2"/>
                <Boundary bc_id="3"/>
                <Boundary bc_id="4"/>
                <Boundary bc_id="5"/>
            </GeoBoundaries>
            <ResBoundaries>
                <Boundary bc_id="2"/>
                <Boundary bc_id="3"/>
                <Boundary bc_id="4"/>
                <Boundary bc_id="5"/>
            </ResBoundaries>            
        </RegionData>          
    </RegionsDefinition>
    <BCUndrainedGeomechanics>
        <UndrainedGeomechanic bc_id="2" type="Ntn" tn="-50.0" />
        <UndrainedGeomechanic bc_id="3" type="Ntn" tn="-50.0" />
        <UndrainedGeomechanic bc_id="4" type="Dux" ux="0.0" />
        <UndrainedGeomechanic bc_id="5" type="Duy" uy="0.0" />
    </BCUndrainedGeomechanics>     
    <BCGeomechanics>
        <Geomechanic bc_id="2" type="Ntn" tn="0.0" />
        <Geomechanic bc_id="3" type="Ntn" tn="-50.0" />
        <Geomechanic bc_id="4" type="Dux" ux="0.0" />
        <Geomechanic bc_id="5" type="Duy" uy="0.0" />
    </BCGeomechanics> 
    <BCReservoirs>
        <Reservoir bc_id="2" type="Dp" p="10.0e6" />
        <Reservoir bc_id="3" type="Dp" p="40.0e6" />
        <Reservoir bc_id="4" type="Dp" p="40.0e6" />
        <Reservoir bc_id="5" type="Dp" p="40.0e6" />
    </BCReservoirs>     
</CaseData>
