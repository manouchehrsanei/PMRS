<?xml version="1.0" ?>
    <!-- Example 1: Simulating poroelastoplastic case -->
<CaseData>
    <!-- Geometry description -->
    <Mesh>
       <MeshFile mesh_file="/Users/omar/Documents/GitHub/PMRS/input/meshes/Con_Wellbore.msh" />
    </Mesh>
    <!-- Time step controls  -->
    <TimeControls>
        <StepSize dt="100" />
        <StepNumber n_time_steps="10" />
        <CrankNicolsonQ useQ="0" />
    </TimeControls>
    <!-- Newton method controls  -->
    <NewtonControls>
        <Iterations n_iterations="50" />
        <Residue res_tolerance="0.001" />
        <Correction cor_tolerance="0.001" />
    </NewtonControls>
    <!-- Fixed Stress Split Scheme  -->
    <FixedStressSplit>
        <FssIterations n_max_fss_iterations="50" />
        <EnfFssIterations n_enforce_fss_iterations="0" />
        <NonlinearAcceleration n_nonlinear_acceleration="None" /> <!-- n_nonlinear_acceleration = {None,FDM,SDM}  -->
        <NState n_state="6" /> <!-- NState = {2,4,6}  -->
        <MaxThetaRatio max_theta_value="0.5" /> <!-- max_theta_value = {0.0-1.0}  -->
    </FixedStressSplit>
    <!-- SubSteps controls  -->
    <SubSteps>
        <MaxPlasticNorm max_plastic_norm_value="0.1" />
    </SubSteps>  
    <!-- Parallel controls  -->
    <ParallelControls>
        <Numthreads n_threads="12" />
    </ParallelControls>
    <!-- Scale factor controls  -->
    <ScaleFactor>
        <Valscalefactor scalfac_value="1.0e-6" />
    </ScaleFactor>    
    <!-- Finite element controls  -->
    <FEM>
        <FullyCoupled useQs="1" />
        <MixedFormulationQ useQ="0" />
        <HRefine h_level="0" />
        <Elasticity p_order="2" />
        <Diffusion p_order="1" />
    </FEM>
    <!-- Settings for OutPut -->
    <OutputControls>
        <OutputFolder name="mkdir Output" />
        <LogFolder name="mkdir Log" />
        <PostProcessing vtk_file="Wellbore_2D_DS" n_divisions="1" n_outputs_geo="5" n_outputs_res="4" draw_geometry_Q="0" performance_summary_Q="1" />
        <OutputControlsGeo>
            <Var v_name="u" />
            <Var t_name="s" />
            <Var t_name="e" />
            <Var t_name="ep" />
            <Var t_name="s_t" />
        </OutputControlsGeo>
        <OutputControlsRes>
            <Var s_name="p" />
            <Var s_name="phi" />
            <Var s_name="kappa" />
            <Var v_name="q" />
        </OutputControlsRes> 
        <ReportingTimes>
            <time t="100" />
            <time t="200" />
            <time t="300" />
            <time t="400" />
            <time t="500" />
            <time t="600" />
            <time t="700" />
            <time t="800" />
            <time t="900" />                                                                        
            <time t="1000" />            
        </ReportingTimes>         
    </OutputControls>
    <Physics>
        <GravityConstant gravity="0.0" />
        <GravityDirection x_direction="0.0" y_direction="0.0" z_direction="-1.0" />
    </Physics>
    <!-- Material Settings -->
    <ReservoirRegions>
        <RegionNumber n_regions="1" />
    </ReservoirRegions> 
    <RegionsDefinition>
        <!-- Region definition -->
        <RegionData mat_id="1" n_boundaries_geo="4" n_boundaries_res ="4">
            <InitialPoroMechParameters Eyoung_u="23775.3" nu_u="0.264778" phi_0="0.132" kappa_0="2.0e-13" />            
            <PoroMechParameters Eyoung="23775.3" nu="0.264778" alpha="1.0" c_f="0.0e-10" eta="0.001" rho_f="1000.0" rho_s="2700.0" />
            <PhiParameters phi_model="Linear" n_parameters="0"/>
            <KappaParameters kappa_model="Constant" n_parameters="0" a="20.0" />
            <PlasticityParameters plasticity_model="DS" n_parameters="7" a="213.01" b="1.723e-3" c="196.30" d="7.084e-4" r="5.03" w="0.102" x0="-480.289"/>    
            <GeoBoundaries>
                <Boundary bc_id="2"/>
                <Boundary bc_id="3"/>
                <Boundary bc_id="4"/>
                <Boundary bc_id="5"/>
            </GeoBoundaries>
            <ResBoundaries>
                <Boundary bc_id="2"/>
                <Boundary bc_id="3"/>
                <Boundary bc_id="4"/>
                <Boundary bc_id="5"/>
            </ResBoundaries>            
        </RegionData>          
    </RegionsDefinition>
    <!-- BCInitialGeomechanics -->
    <BCInitialGeomechanics>
        <InitialGeomechanic bc_id="2" type="Ntn" n_data="1" >
            <Data t="0" tn="-90." />
        </InitialGeomechanic>            
        <InitialGeomechanic bc_id="3" type="Ntn" n_data="1" >
            <Data t="0" tn="-90.0" />
        </InitialGeomechanic>
        <InitialGeomechanic bc_id="4" type="Dux" n_data="1" >
            <Data t="0" ux="0.0" />            
        </InitialGeomechanic>
        <InitialGeomechanic bc_id="5" type="Duy" n_data="1" >
            <Data t="0" uy="0.0" />            
        </InitialGeomechanic>            
    </BCInitialGeomechanics>
    <BCInitialReservoir>
        <InitialReservoir bc_id="2" type="Dp" n_data="1" >
            <Data t="0" p="40.0" />              
        </InitialReservoir>
        <InitialReservoir bc_id="3" type="Dp" n_data="1" >
            <Data t="0" p="40.0" />             
        </InitialReservoir>
        <InitialReservoir bc_id="4" type="Dp" n_data="1" >
            <Data t="0" p="40.0" />             
        </InitialReservoir>            
        <InitialReservoir bc_id="5" type="Dp" n_data="1" >
            <Data t="0" p="40.0" />             
        </InitialReservoir>            
    </BCInitialReservoir>  
    <!-- Recurrent boundary contidions -->    
    <BCGeomechanics>
        <Geomechanic bc_id="2" type="Ntn" n_data="3" >
            <Data t="0" tn="40.0" />
            <Data t="100" tn="0.0" />
            <Data t="1000" tn="0.0" />
        </Geomechanic>
        <Geomechanic bc_id="3" type="Ntn" n_data="3" >
            <Data t="0" tn="0.0001" />
            <Data t="100" tn="0.0" />
            <Data t="1000" tn="0.0" />
        </Geomechanic>
        <Geomechanic bc_id="4" type="Dux" n_data="1" >
            <Data t="0" ux="0.0" />
        </Geomechanic>
        <Geomechanic bc_id="5" type="Duy" n_data="1" >
            <Data t="0" uy="0.0" />               
        </Geomechanic>
    </BCGeomechanics> 
    <BCReservoirs>
        <Reservoir bc_id="2" type="Dp" n_data="3" >
            <Data t="0" p="40.0" />
            <Data t="100" p="20.0" />
            <Data t="1000" p="20.0" />
        </Reservoir>
        <Reservoir bc_id="3" type="Dp" n_data="3" >
            <Data t="0" p="40.0" />
            <Data t="100" p="40.0" />
            <Data t="1000" p="40.0" />
        </Reservoir>            
        <Reservoir bc_id="4" type="Dp" n_data="1" >
            <Data t="0" p="40.0" />
        </Reservoir>            
        <Reservoir bc_id="5" type="Dp" n_data="1" >
            <Data t="0" p="40.0" />  
        </Reservoir>            
    </BCReservoirs>       
</CaseData>
