<?xml version="1.0" ?>
    <!-- Example 1: Simulating poroelastoplastic case -->
<CaseData>
    <!-- Geometry description -->
    <Mesh>
       <MeshFile mesh_file="/Users/omar/Documents/GitHub/PMRS/input/meshes/HorizontalWellbore.msh" />
    </Mesh>
    <!-- Time step controls  -->
    <TimeControls>
        <StepSize dt="1" />
        <StepNumber n_time_steps="10" />
        <CrankNicolsonQ useQ="1" />
    </TimeControls>
    <!-- Newton method controls  -->
    <NewtonControls>
        <Iterations n_iterations="50" />
        <Residue res_tolerance="0.01" />
        <Correction cor_tolerance="1.0" />
    </NewtonControls>
    <!-- Fixed Stress Split Scheme  -->
    <FixedStressSplit>
        <FssIterations n_max_fss_iterations="50" />
        <EnfFssIterations n_enforce_fss_iterations="0" />
        <NonlinearAcceleration nonlinear_acceleration="Aitken" />
    </FixedStressSplit>
    <!-- Parallel controls  -->
    <ParallelControls>
        <Numthreads n_threads="12" />
    </ParallelControls>
    <!-- Scale factor controls  -->
    <ScaleFactor>
        <Valscalefactor scalfac_value="1.0e-6" />
    </ScaleFactor>    
    <!-- Finite element controls  -->
    <FEM>
        <MixedFormulationQ useQ="0" />
        <HRefine h_level="0" />
        <Elasticity p_order="1" />
        <Diffusion p_order="1" />
    </FEM>
    <!-- Settings for OutPut -->
    <OutputControls>
        <OutputFolder name="mkdir Output" />
        <LogFolder name="mkdir Log" />
        <PostProcessing vtk_file="Wellbore_3D_CG" n_divisions="0" n_outputs_geo="4" n_outputs_res="5" />
        <OutputControlsGeo>
            <Var v_name="u" />
            <Var t_name="s" />
            <Var t_name="e" />
            <Var t_name="ep" />
        </OutputControlsGeo>
        <OutputControlsRes>
            <Var s_name="p" />
            <Var s_name="phi" />
            <Var s_name="kappa" />
            <Var s_name="div_q" />
            <Var v_name="q" />
        </OutputControlsRes>        
    </OutputControls>
    <Physics>
