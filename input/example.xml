<?xml version="1.0" ?>
    <!-- Example 1: Simulating an poroelastic example  -->
<CaseData>
    <!-- Geometry description -->
    <Mesh>
       <MeshFile mesh_file="/Users/manouchehr/Documents/GitHub/PMRS/input/meshes/plug.msh" />
    </Mesh>
    <!-- Time step controls  -->
    <TimeControls>
        <StepSize dt="1.0e6" />
        <StepNumber n_time_steps="1" />
    </TimeControls>
    <!-- Newton method controls  -->
    <NewtonControls>
        <Iterations n_iterations="10" />
        <Residue res_tolerance="0.01" />
        <Correction cor_tolerance="0.01" />
    </NewtonControls> 
    <!-- Parallel controls  -->
    <ParallelControls>
        <Numthreads n_threads="4" />
    </ParallelControls> 
    <!-- Finite element controls  -->
    <FEM>
        <MixedFormulationQ useQ="0" />
        <HRefine h_level="0" />
        <Elasticity p_order="2" />
        <Diffusion p_order="1" />
    </FEM>
    <!-- Settings for OutPut -->
    <OutputControls>
        <OutputFolder name="mkdir Output" />
        <LogFolder name="mkdir Log" />
        <PostProcessing vtk_file="plug.vtk" n_divisions="0" n_scalars="35" n_vectors="2"
        />
        <Scalars>
            <Var name="et_v" />
            <Var name="et_x" />
            <Var name="et_y" />
            <Var name="et_z" />
            <Var name="et_xy" />
            <Var name="et_xz" />
            <Var name="et_yz" />
            <Var name="e_v" />
            <Var name="e_x" />
            <Var name="e_y" />
            <Var name="e_z" />
            <Var name="e_xy" />
            <Var name="e_xz" />
            <Var name="e_yz" />
            <Var name="ep_v" />
            <Var name="ep_x" />
            <Var name="ep_y" />
            <Var name="ep_z" />
            <Var name="ep_xy" />
            <Var name="ep_xz" />
            <Var name="ep_yz" />
            <Var name="p" />
            <Var name="phi" />
            <Var name="k_x" />
            <Var name="k_y" />
            <Var name="k_z" />
            <Var name="s_x" />
            <Var name="s_y" />
            <Var name="s_z" />
            <Var name="t_xy" />
            <Var name="t_xz" />
            <Var name="t_yz" />
            <Var name="K_0" />
            <Var name="YS_1" />
            <Var name="YS_2" />
            <Var name="YS_3" />
        </Scalars>
        <Vectors>
            <Var name="u" />
            <Var name="v" />
        </Vectors>
    </OutputControls>
    <Physics>
        <GravityConstant gravity="0.0" />
        <GravityDirection x_direction="0.0" y_direction="0.0" z_direction="-1.0" />
        <Prestress sxx= "0.0" syy= "0.0" szz= "0.0" sxy= "0.0" sxz= "0.0" syz= "0.0" />
    </Physics>
    <!-- Material Settings -->
    <ReservoirRegions>
        <RegionNumber n_regions="1" />
    </ReservoirRegions> 
    <RegionsDefinition>
        <!-- Region definition -->
        <RegionData mat_id="1" n_boundaries="3" n_parameters="12" >
            <Parameters eyoung="1.0e9" nu="0.3" phi="0.1" kappa="1.0e-13" alpha="1.0" m="1.0e19" mu="0.001" rhof="1000.0" rhos="2700.0"
            MCcoh="0.0" MCphi="0.0" MCpsi="0.0"/>
            <Boundaries>
                <Boundary bc_id="2"/>
                <Boundary bc_id="3"/>
                <Boundary bc_id="4"/>
            </Boundaries>
        </RegionData>          
    </RegionsDefinition>
    <BoundaryConditions>
        <BoundaryCondition bc_id="2" type="Duy_Nq" uy="0.0" qn="0.0" />
        <BoundaryCondition bc_id="3" type="Nt_Dp" tx="0.0" ty="-1.0e6" p="0.0" />
        <BoundaryCondition bc_id="4" type="Dux_Nq" ux="0.0" qn="0.0" />
    </BoundaryConditions>           
</CaseData>
