<?xml version="1.0" ?>
    <!-- Example 1: Simulating an Poroelasticplastic  -->
<CaseData>
    <!-- Geometry description -->
    <Mesh>
       <MeshFile mesh_file="/Users/manouchehr/Documents/GitHub/PMRS/input/meshes/CoupPoroPerm.msh" />
    </Mesh>
    <!-- Time step controls  -->
    <TimeControls>
        <StepSize dt="10e6" />
        <StepNumber n_time_steps="1" />
        <CrankNicolsonQ useQ="0" />
    </TimeControls>
    <!-- Newton method controls  -->
    <NewtonControls>
        <Iterations n_iterations="40" />
        <Residue res_tolerance="0.001" />
        <Correction cor_tolerance="0.001" />
    </NewtonControls>
        <!-- Fixed Stress Split Scheme  -->
    <FixedStressSplit>
        <FssIterations n_max_fss_iterations="20" />
        <EnfFssIterations n_enforce_fss_iterations="5" />
    </FixedStressSplit>
    <!-- Parallel controls  -->
    <ParallelControls>
        <Numthreads n_threads="0" />
    </ParallelControls>
    <!-- Scale factor controls  -->
    <ScaleFactor>
        <Valscalefactor scalfac_value="1.0e-6" />
    </ScaleFactor>   
    <!-- Finite element controls  -->
    <FEM>
        <MixedFormulationQ useQ="0" />
        <HRefine h_level="0" />
        <Elasticity p_order="2" />
        <Diffusion p_order="1" />
    </FEM>
    <!-- Settings for OutPut -->
    <OutputControls>
        <OutputFolder name="mkdir Output" />
        <LogFolder name="mkdir Log" />
        <PostProcessing vtk_file="Poroelasticplastic" n_divisions="1" n_outputs_geo="7" n_outputs_res="1" />
        <OutputControlsGeo>
            <Var s_name="e_x" />
            <Var s_name="e_y" />
            <Var s_name="phi" />
            <Var s_name="k_x" />
            <Var s_name="k_y" />
            <Var s_name="s_x" />
            <Var s_name="s_y" />
        </OutputControlsGeo>
        <OutputControlsRes>
            <Var s_name="p" />
        </OutputControlsRes>        
    </OutputControls>
    <Physics>
        <GravityConstant gravity="0.0" />
        <GravityDirection x_direction="0.0" y_direction="0.0" z_direction="-1.0" />
    </Physics>
    <!-- Material Settings -->
    <ReservoirRegions>
        <RegionNumber n_regions="1" />
    </ReservoirRegions> 
    <RegionsDefinition>
        <!-- Region definition -->
        <RegionData mat_id="1" n_boundaries_geo="3" n_boundaries_res ="3">
            <PoroMechUndrainedParameters Eyoung_u="100" nu_u="0.2" phi_0="0.25" kappa_0="5.0e-9" />            
            <PoroMechParameters Eyoung="4.36" nu="0.2" alpha="1.0" Se="1.0e-29" eta="0.001" rho_f="1000.0" rho_s="2700.0" c_f="0.0" />
            <PhiParameters phi_model="Linear" n_parameters="0"/>
            <KappaParameters kappa_model="Constant" n_parameters="0"/>
            <PlasticityParameters plasticity_model="MC" n_parameters="2" cohesion="0.0" friction="0.0"/>            
            <GeoBoundaries>
                <Boundary bc_id="2"/>
                <Boundary bc_id="3"/>
                <Boundary bc_id="4"/>
            </GeoBoundaries>
            <ResBoundaries>
                <Boundary bc_id="2"/>
                <Boundary bc_id="3"/>
                <Boundary bc_id="4"/>
            </ResBoundaries>            
        </RegionData>          
    </RegionsDefinition>
    <BCUndrainedGeomechanics>
        <UndrainedGeomechanic bc_id="2" type="Duy" uy="0.0" />
        <UndrainedGeomechanic bc_id="3" type="Nt" tx="0.0" ty="0.0" />
        <UndrainedGeomechanic bc_id="4" type="Dux" ux="0.0" />
    </BCUndrainedGeomechanics>     
    <BCGeomechanics>
        <Geomechanic bc_id="2" type="Duy" uy="0.0" />
        <Geomechanic bc_id="3" type="Nt" tx="0.0" ty="-5.0" />
        <Geomechanic bc_id="4" type="Dux" ux="0.0" />
    </BCGeomechanics> 
    <BCReservoirs>
        <Reservoir bc_id="2" type="Dp" p="0.0" />
        <Reservoir bc_id="3" type="Dp" p="2.0" />
        <Reservoir bc_id="4" type="Nq" qn="0.0" />
    </BCReservoirs>     
</CaseData>
