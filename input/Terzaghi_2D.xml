<?xml version="1.0" ?>
    <!-- Example 1: Simulating poroelastoplastic case -->
<CaseData>
    <!-- Geometry description -->
    <Mesh>
       <MeshFile mesh_file="/Users/omar/Documents/GitHub/PMRS/input/meshes/Terzaghi_2D.msh" />
    </Mesh>
    <!-- Time step controls  -->
    <TimeControls>
        <StepSize dt="0.01" />
        <StepNumber n_time_steps="10" />
        <CrankNicolsonQ useQ="0" />
    </TimeControls>
    <!-- Newton method controls  -->
    <NewtonControls>
        <Iterations n_iterations="50" />
        <Residue res_tolerance="0.000001" />
        <Correction cor_tolerance="0.00001" />
    </NewtonControls>
    <!-- Fixed Stress Split Scheme  -->
    <FixedStressSplit>
        <FssIterations n_max_fss_iterations="20" />
        <EnfFssIterations n_enforce_fss_iterations="0" />
    </FixedStressSplit>
    <!-- Parallel controls  -->
    <ParallelControls>
        <Numthreads n_threads="0" />
    </ParallelControls>
    <!-- Scale factor controls  -->
    <ScaleFactor>
        <Valscalefactor scalfac_value="1.0e-6" />
    </ScaleFactor>    
    <!-- Finite element controls  -->
    <FEM>
        <MixedFormulationQ useQ="0" />
        <HRefine h_level="0" />
        <Elasticity p_order="2" />
        <Diffusion p_order="1" />
    </FEM>
    <!-- Settings for OutPut -->
    <OutputControls>
        <OutputFolder name="mkdir Output" />
        <LogFolder name="mkdir Log" />
        <PostProcessing vtk_file="Terzaghi_2D_CG" n_divisions="1" n_outputs_geo="4" n_outputs_res="5" />
        <OutputControlsGeo>
            <Var v_name="u" />
            <Var t_name="s" />
            <Var t_name="e" />
            <Var t_name="ep" />
        </OutputControlsGeo>
        <OutputControlsRes>
            <Var s_name="p" />
            <Var s_name="phi" />
            <Var s_name="kappa" />
            <Var s_name="div_q" />
            <Var v_name="q" />
        </OutputControlsRes>        
    </OutputControls>
    <Physics>
        <GravityConstant gravity="0.0" />
        <GravityDirection x_direction="0.0" y_direction="0.0" z_direction="-1.0" />
    </Physics>
    <!-- Material Settings -->
    <ReservoirRegions>
        <RegionNumber n_regions="1" />
    </ReservoirRegions> 
    <RegionsDefinition>
        <!-- Region definition -->
        <RegionData mat_id="1" n_boundaries_geo="3" n_boundaries_res ="3">
            <PoroMechUndrainedParameters Eyoung_u="1000.0" nu_u="0.4999" phi_0="0.1" kappa_0="1.0e-13" />            
            <PoroMechParameters Eyoung="1000.0" nu="0.2" alpha="1.0" c_f="0.0e-10" eta="0.001" rho_f="1000.0" rho_s="2500.0" />
            <PhiParameters phi_model="Linear" n_parameters="0"/>
            <KappaParameters kappa_model="Petunin" n_parameters="1" a="20.0" />
            <PlasticityParameters plasticity_model="MC" n_parameters="1" cohesion="10.0" friction="10.0"/>            
            <GeoBoundaries>
                <Boundary bc_id="2"/>
                <Boundary bc_id="3"/>
                <Boundary bc_id="4"/>
            </GeoBoundaries>
            <ResBoundaries>
                <Boundary bc_id="2"/>
                <Boundary bc_id="3"/>
                <Boundary bc_id="4"/>
            </ResBoundaries>            
        </RegionData>          
    </RegionsDefinition>
    <BCUndrainedGeomechanics>
        <UndrainedGeomechanic bc_id="2" type="Duy" uy="0.0" />
        <UndrainedGeomechanic bc_id="3" type="Ntn" tn="-10.0" />
        <UndrainedGeomechanic bc_id="4" type="Dux" ux="0.0" />
    </BCUndrainedGeomechanics>     
    <BCGeomechanics>
        <Geomechanic bc_id="2" type="Duy" uy="0.0" />
        <Geomechanic bc_id="3" type="Ntn" tn="0.0" />
        <Geomechanic bc_id="4" type="Dux" ux="0.0" />
    </BCGeomechanics> 
    <BCReservoirs>
        <Reservoir bc_id="2" type="Nq" qn="0.0" />
        <Reservoir bc_id="3" type="Dp" p="0.0" />
        <Reservoir bc_id="4" type="Nq" qn="0.0" />
    </BCReservoirs>     
</CaseData>
