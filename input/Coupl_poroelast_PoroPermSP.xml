<?xml version="1.0" ?>
    <!-- Example 1: Simulating an CouplPoroelastPoroPermP_pore pressure and S_stress -->
<CaseData>
    <!-- Geometry description -->
    <Mesh>
       <MeshFile mesh_file="/Users/omar/Documents/GitHub/PMRS/input/meshes/CoupPoroPerm.msh" />
    </Mesh>
    <!-- Time step controls  -->
    <TimeControls>
        <StepSize dt="0.0001" />
        <StepNumber n_time_steps="10" />
    </TimeControls>
    <!-- Newton method controls  -->
    <NewtonControls>
        <Iterations n_iterations="40" />
        <Residue res_tolerance="0.00001" />
        <Correction cor_tolerance="0.001" />
    </NewtonControls>
    <!-- Parallel controls  -->
    <ParallelControls>
        <Numthreads n_threads="0" />
    </ParallelControls>  
    <!-- Finite element controls  -->
    <FEM>
        <MixedFormulationQ useQ="0" />
        <HRefine h_level="0" />
        <Elasticity p_order="2" />
        <Diffusion p_order="1" />
    </FEM>
    <!-- Settings for OutPut -->
    <OutputControls>
        <OutputFolder name="mkdir Output" />
        <LogFolder name="mkdir Log" />
        <PostProcessing vtk_file="CouplPoroelastPoroPermSP.vtk" n_divisions="0" n_reservoiroutputs="1" n_geomechanicoutputs="7"
        />
        <ReservoirOutputControls>
            <Var name="p" />
        </ReservoirOutputControls>
        <GeomechanicOutputControls>
            <Var name="e_x" />
            <Var name="e_y" />
            <Var name="phi" />
            <Var name="k_x" />
            <Var name="k_y" />
            <Var name="s_x" />
            <Var name="s_y" />
        </GeomechanicOutputControls>
    </OutputControls>
    <Physics>
        <GravityConstant gravity="0.0" />
        <GravityDirection x_direction="0.0" y_direction="0.0" z_direction="-1.0" />
        <Prestress sxx= "0.0" syy= "0.0" szz= "0.0" sxy= "0.0" sxz= "0.0" syz= "0.0" />
    </Physics>
    <!-- Material Settings -->
    <ReservoirRegions>
        <RegionNumber n_regions="1" />
    </ReservoirRegions> 
    <RegionsDefinition>
        <!-- Region definition -->
        <RegionData mat_id="1" n_boundaries="3" n_parameters="12" >
            <Parameters eyoung="43457.2" nu="0.357983" phi="0.25" kappa="5.0e-9" alpha="1.0" m="1.0e29" mu="0.001" rhof="1000.0" rhos="2700.0"
            MCcoh="1e6" MCphi="25.0" MCpsi="25.0"/>
            <Boundaries>
                <Boundary bc_id="2"/>
                <Boundary bc_id="3"/>
                <Boundary bc_id="4"/>
            </Boundaries>
        </RegionData>          
    </RegionsDefinition>
    <BoundaryConditionReservoirs>
        <BoundaryConditionReservoir bc_id="2" type="Dp" p="20.0e6" />
        <BoundaryConditionReservoir bc_id="3" type="Dp" p="0.0e6" />
        <BoundaryConditionReservoir bc_id="4" type="Nq" qn="0.0" />
    </BoundaryConditionReservoirs> 
    <BoundaryConditionGeomechanics>
        <BoundaryConditionGeomechanic bc_id="2" type="Duz"  uz="0.0" />
        <BoundaryConditionGeomechanic bc_id="3" type="Nt" tx="0.0" ty="0.0" tz="-10.0" />
        <BoundaryConditionGeomechanic bc_id="4" type="Duxy" ux="0.0" uy="0.0"  />
    </BoundaryConditionGeomechanics> 
</CaseData>
