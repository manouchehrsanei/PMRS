<?xml version="1.0" ?>
    <!-- Example 1: Simulating poroelastoplastic case -->
<CaseData>
    <!-- Geometry description -->
    <Mesh>
       <MeshFile mesh_file="/Users/omar/Documents/GitHub/PMRS/input/meshes/Terzaghi_2D.msh" />
    </Mesh>
    <!-- Time step controls  -->
    <TimeControls>
        <StepSize dt="0.01" />
        <StepNumber n_time_steps="10" />
        <CrankNicolsonQ useQ="0" />
    </TimeControls>
    <!-- Newton method controls  -->
    <NewtonControls>
        <Iterations n_iterations="40" />
        <Residue res_tolerance="0.00001" />
        <Correction cor_tolerance="0.00000001" />
    </NewtonControls>
    <!-- Parallel controls  -->
    <ParallelControls>
        <Numthreads n_threads="0" />
    </ParallelControls>  
    <!-- Finite element controls  -->
    <FEM>
        <MixedFormulationQ useQ="1" />
        <HRefine h_level="0" />
        <Elasticity p_order="1" />
        <Diffusion p_order="1" />
    </FEM>
    <!-- Settings for OutPut -->
    <OutputControls>
        <OutputFolder name="mkdir Output" />
        <LogFolder name="mkdir Log" />
        <PostProcessing vtk_file="Wellbore_Cricular_2D.vtk" n_divisions="1" n_outputs_geo="7" n_outputs_res="1" />
        <OutputControlsGeo>
            <Var s_name="e_x" />
            <Var s_name="e_y" />
            <Var s_name="phi" />
            <Var s_name="k_x" />
            <Var s_name="k_y" />
            <Var s_name="s_x" />
            <Var s_name="s_y" />
        </OutputControlsGeo>
        <OutputControlsRes>
            <Var s_name="p" />
        </OutputControlsRes>        
    </OutputControls>
    <Physics>
        <GravityConstant gravity="0.0" />
        <GravityDirection x_direction="0.0" y_direction="0.0" z_direction="-1.0" />
    </Physics>
    <!-- Material Settings -->
    <ReservoirRegions>
        <RegionNumber n_regions="1" />
    </ReservoirRegions> 
    <RegionsDefinition>
        <!-- Region definition -->
        <RegionData mat_id="1" n_boundaries_geo="3" n_boundaries_res ="3" n_poro_mech_undrained_par="4" n_poro_mech_par="9" n_plasticity_par ="3">
            <PoroMechUndrainedParameters Eyoung_u="29269.0" nu_u="0.20300" phi_0="0.25" kappa_0="5.0e-9" />            
            <PoroMechParameters Eyoung="29269.0" nu="0.20300" alpha="1.0" M="1.0e29" eta="0.001" rho_f="1000.0" rho_s="2700.0" phi_model="phi1" kappa_model="kappa1" />
            <PoroMechParameters Eyoung="29269.0" nu="0.20300" alpha="1.0" M="1.0e29" eta="0.001" rho_f="1000.0" rho_s="2700.0" phi_model="phi1" kappa_model="kappa1" />
            <PlasticityParameters name="MC" cohesion="0.20300" friction="0.25"/>            
            <GeoBoundaries>
                <Boundary bc_id="2"/>
                <Boundary bc_id="3"/>
                <Boundary bc_id="4"/>
            </GeoBoundaries>
            <ResBoundaries>
                <Boundary bc_id="2"/>
                <Boundary bc_id="3"/>
                <Boundary bc_id="4"/>
            </ResBoundaries>            
        </RegionData>          
    </RegionsDefinition>
    <BoundaryConditionReservoirs>
        <BoundaryConditionReservoir bc_id="2" type="Nq" qn="0.0" />
        <BoundaryConditionReservoir bc_id="3" type="Dp" p="0.0" />
        <BoundaryConditionReservoir bc_id="4" type="Nq" qn="0.0" />
    </BoundaryConditionReservoirs> 
    <BoundaryConditionUndrainedGeomechanics>
        <BoundaryConditionUndrainedGeomechanic bc_id="2" type="Duy" uy="0.0" />
        <BoundaryConditionUndrainedGeomechanic bc_id="3" type="Ntn" tn="-100.0" />
        <BoundaryConditionUndrainedGeomechanic bc_id="4" type="Dux" ux="0.0" />
    </BoundaryConditionUndrainedGeomechanics>     
    <BoundaryConditionGeomechanics>
        <BoundaryConditionGeomechanic bc_id="2" type="Duy" uy="0.0" />
        <BoundaryConditionGeomechanic bc_id="3" type="Ntn" tn="-100.0" />
        <BoundaryConditionGeomechanic bc_id="4" type="Dux" ux="0.0" />
    </BoundaryConditionGeomechanics> 
</CaseData>
