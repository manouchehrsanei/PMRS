<?xml version="1.0" ?>
    <!-- Example 1: Simulating HydrostTest_Collapse Prodution well case -->
<CaseData>
    <!-- Geometry description -->
    <Mesh>
       <MeshFile mesh_file="/Users/manouchehr/Documents/GitHub/PMRS/input/meshes/collapseWell.msh" />
    </Mesh>
    <!-- Time step controls  -->
    <TimeControls>
        <StepSize dt="10000000" />
        <StepNumber n_time_steps="1" />
        <CrankNicolsonQ useQ="0" />
    </TimeControls>
    <!-- Newton method controls  -->
    <NewtonControls>
        <Iterations n_iterations="40" />
        <Residue res_tolerance="0.000001" />
        <Correction cor_tolerance="0.000001" />
    </NewtonControls>
        <!-- Fixed Stress Split Scheme  -->
    <FixedStressSplit>
        <FssIterations n_max_fss_iterations="20" />
        <EnfFssIterations n_enforce_fss_iterations="5" />
    </FixedStressSplit>
    <!-- Parallel controls  -->
    <ParallelControls>
        <Numthreads n_threads="0" />
    </ParallelControls>
    <!-- Scale factor controls  -->
    <ScaleFactor>
        <Valscalefactor scalfac_value="1.0e-6" />
    </ScaleFactor>    
    <!-- Finite element controls  -->
    <FEM>
        <MixedFormulationQ useQ="0" />
        <HRefine h_level="0" />
        <Elasticity p_order="2" />
        <Diffusion p_order="1" />
    </FEM>
    <!-- Settings for OutPut -->
    <OutputControls>
        <OutputFolder name="mkdir Output" />
        <LogFolder name="mkdir Log" />
        <PostProcessing vtk_file="Collapse_WellProduct" n_divisions="1" n_outputs_geo="3" n_outputs_res="2" />
        <OutputControlsGeo>
            <Var s_name="ux" />
            <Var s_name="exx" />
            <Var s_name="sxx" />
        </OutputControlsGeo>
        <OutputControlsRes>
            <Var s_name="p" />
            <Var s_name="phi" />
        </OutputControlsRes>        
    </OutputControls>
    <Physics>
        <GravityConstant gravity="0.0" />
        <GravityDirection x_direction="0.0" y_direction="0.0" z_direction="-1.0" />
    </Physics>
    <!-- Material Settings -->
    <ReservoirRegions>
        <RegionNumber n_regions="1" />
    </ReservoirRegions> 
    <RegionsDefinition>
        <!-- Region definition -->
        <RegionData mat_id="1" n_boundaries_geo="4" n_boundaries_res ="4">
            <PoroMechUndrainedParameters Eyoung_u="23775.3" nu_u="0.264778" phi_0="0.132" kappa_0="2.0e-15" />            
            <PoroMechParameters Eyoung="23775.3" nu="0.264778" alpha="1.0" Se="0.0e-10" eta="0.001" rho_f="1000.0" rho_s="2698.0" c_f="0.0" />
            <PhiParameters phi_model="Linear" n_parameters="0"/>
            <KappaParameters kappa_model="Constant" n_parameters="0" />
            <PlasticityParameters plasticity_model="DS" n_parameters="7" a="213.01" b="1.723e-3" c="196.30" d="7.084e-4" r="5.03" w="0.102" x0="-480.289"/>
            <GeoBoundaries>
                <Boundary bc_id="2"/>
                <Boundary bc_id="3"/>
                <Boundary bc_id="4"/>
                <Boundary bc_id="5"/>
            </GeoBoundaries>
            <ResBoundaries>
                <Boundary bc_id="2"/>
                <Boundary bc_id="3"/>
                <Boundary bc_id="4"/>
                <Boundary bc_id="5"/>
            </ResBoundaries>            
        </RegionData>          
    </RegionsDefinition>
    <BCUndrainedGeomechanics>
        <UndrainedGeomechanic bc_id="2" type="Ntn" tn="0.0" />
        <UndrainedGeomechanic bc_id="3" type="Ntn" tn="0.0" />
        <UndrainedGeomechanic bc_id="4" type="Dux" ux="0.0" />
        <UndrainedGeomechanic bc_id="5" type="Duy" uy="0.0" />
    </BCUndrainedGeomechanics>     
    <BCGeomechanics>
        <Geomechanic bc_id="2" type="Ntn" tn="30.0" />
        <Geomechanic bc_id="3" type="Ntn" tn="-130.0" />
        <Geomechanic bc_id="4" type="Dux" ux="0.0" />
        <Geomechanic bc_id="5" type="Duy" uy="0.0" />
    </BCGeomechanics> 
    <BCReservoirs>
        <Reservoir bc_id="2" type="Dp" p="30.0e6" />
        <Reservoir bc_id="3" type="Dp" p="50.0e6" />
        <Reservoir bc_id="4" type="Dp" p="50.0e6" />
        <Reservoir bc_id="5" type="Dp" p="50.0e6" />
    </BCReservoirs>        
</CaseData>
